#include "Sorcerer.hh"
#include "Victim.hh"
#include "Peon.hh"
#include <string>
#include <iostream>

int main()
{
  Sorcerer robert("Robert", "the Magnificent");

  Victim jim("Jimmy");
  Peon joe("Joe");

  std::cout << robert << jim << joe;

  robert.polymorph(jim);
  robert.polymorph(joe);

  return 0;
}
